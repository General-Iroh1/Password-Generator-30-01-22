One = False
Two = False
Three = False
Four = False
if One == True:
    with open("Password Saver.rtf",'r') as file:
        lines = file.readlines()
    with open("Password Saver.rtf",'w') as file:
        for line in file:
        # find() returns -1 if no match is found
            if line.find("You have ran the code") != -1:
                pass
            else:
                file.write(line)
                Two == True
